public class AdPunishment extends BaseDimension implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 721163444653077789L;
	
	/**
	 * 作出行政处罚决定日期
	 */
	private String penalty_decisiondate;
	/**
	 * 作出行政处罚决定机关名称
	 */
	private String penalty_decisioninsititution;
	/**
	 * 行政处罚决定书文号
	 */
	private String penalty_code;
	/**
	 * 违法行为类型
	 */
	private String penalty_illegaltype;
	/**
	 * 行政处罚内容
	 */
	private String penalty_decisioncontent; 
	/**
	 * 事件等级
	 */
	private String level;
	public String getPenalty_decisiondate() {
		return penalty_decisiondate;
	}
	public void setPenalty_decisiondate(String penalty_decisiondate) {
		this.penalty_decisiondate = penalty_decisiondate;
	}
	public String getPenalty_decisioninsititution() {
		return penalty_decisioninsititution;
	}
	public void setPenalty_decisioninsititution(String penalty_decisioninsititution) {
		this.penalty_decisioninsititution = penalty_decisioninsititution;
	}
	public String getPenalty_code() {
		return penalty_code;
	}
	public void setPenalty_code(String penalty_code) {
		this.penalty_code = penalty_code;
	}
	public String getPenalty_illegaltype() {
		return penalty_illegaltype;
	}
	public void setPenalty_illegaltype(String penalty_illegaltype) {
		this.penalty_illegaltype = penalty_illegaltype;
	}
	public String getPenalty_decisioncontent() {
		return penalty_decisioncontent;
	}
	public void setPenalty_decisioncontent(String penalty_decisioncontent) {
		this.penalty_decisioncontent = penalty_decisioncontent;
	}
	public String getLevel() {
		return level;
	}
	public void setLevel(String level) {
		this.level = level;
	}
	     

	

}
