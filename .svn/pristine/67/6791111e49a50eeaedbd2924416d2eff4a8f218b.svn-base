package com.srd.ljzd.dto.law;

/** 
 * @ClassName: LawLegalInstrumentDTO
 * @Description: 法律文书DTO
 * @author shiyong
 * @date 2016年5月12日 下午4:59:42
 *  
 */
public class LawLegalInstrumentDTO {
	private String companyName;
	private String instrumentTypes;
	private String trialClasses;
	private String litigationTypes;
	private String relatedPositions;
	private String causeAction;
	private String beginDate;
	private String endDate;
	private String minAmountCount;
	private String maxAmountCount;
	
	public String getCompanyName() {
		return companyName;
	}
	public void setCompanyName(String companyName) {
		this.companyName = companyName;
	}
	public String getInstrumentTypes() {
		return instrumentTypes;
	}
	public void setInstrumentTypes(String instrumentTypes) {
		this.instrumentTypes = instrumentTypes;
	}
	public String getTrialClasses() {
		return trialClasses;
	}
	public void setTrialClasses(String trialClasses) {
		this.trialClasses = trialClasses;
	}
	public String getLitigationTypes() {
		return litigationTypes;
	}
	public void setLitigationTypes(String litigationTypes) {
		this.litigationTypes = litigationTypes;
	}
	public String getRelatedPositions() {
		return relatedPositions;
	}
	public void setRelatedPositions(String relatedPositions) {
		this.relatedPositions = relatedPositions;
	}
	public String getCauseAction() {
		return causeAction;
	}
	public void setCauseAction(String causeAction) {
		this.causeAction = causeAction;
	}
	public String getBeginDate() {
		return beginDate;
	}
	public void setBeginDate(String beginDate) {
		this.beginDate = beginDate;
	}
	public String getEndDate() {
		return endDate;
	}
	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}
	public String getMinAmountCount() {
		return minAmountCount;
	}
	public void setMinAmountCount(String minAmountCount) {
		this.minAmountCount = minAmountCount;
	}
	public String getMaxAmountCount() {
		return maxAmountCount;
	}
	public void setMaxAmountCount(String maxAmountCount) {
		this.maxAmountCount = maxAmountCount;
	}
	
	
}
