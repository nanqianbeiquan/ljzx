.overlap .lap_mask{position:fixed;top:0px;left:0px;z-index:2000;width:100%;height:100%;background:#000000;opacity:0.2; -moz-opacity:0.2; filter:alpha(opacity=20);}
.overlap .lock_mask{position:fixed;top:0px;left:0px;z-index:6000;width:100%;height:100%;background:#ffffff;opacity:0.0; -moz-opacity:0.0; filter:alpha(opacity=0);cursor:default}
.overlap .load_mask{position:fixed;top:0px;left:0px;z-index:7000;width:100%;height:100%;cursor:default;text-align: center;}
.overlap .lap_block .lap_content{margin:0 auto;z-index:5001;position:fixed;top:0;margin:0 auto;background:#ffffff; 
-moz-background-clip: border; /* Firefox 3.6 */ 
-webkit-background-clip: border; /* Safari 4? Chrome 6? */ 
background-clip: border-box; /* Firefox 4, Safari 5, Opera 10, IE 9 */ 
-moz-background-clip: padding; /* Firefox 3.6 */ 
-webkit-background-clip: padding; /* Safari 4? Chrome 6? */ 
background-clip: padding-box; /* Firefox 4, Safari 5, Opera 10, IE 9 */ 
-moz-background-clip: content; /* Firefox 3.6 */ 
-webkit-background-clip: content; /* Safari 4? Chrome 6? */ 
background-clip: content-box; /* Firefox 4, Safari 5, Opera 10, IE 9 */ }
.overlap .lap_block .lap_content .lap_close_block{height:0px;position:relative;text-align: right;z-index:120;}
.overlap .lap_block .lap_content .lap_close_block .lap_close{cursor:pointer;width:38px;height:38px;text-align: center;background:url(../../images/common/overlap_close.png)no-repeat center;}

.lap_close_block .lap_close .close_left{display:none;width:18px;height:2px;position:relative;top:0px;top:6px;
transform:rotate(45deg);
-ms-transform:rotate(45deg); 	/* IE 9 */
-moz-transform:rotate(45deg); 	/* Firefox */
-webkit-transform:rotate(45deg); /* Safari 和 Chrome */
-o-transform:rotate(45deg); 	/* Opera */}
.lap_close_block .lap_close .close_right{display:none;width:18px;height:2px;position:relative;left:0px;top:-13px;
transform:rotate(135deg);
-ms-transform:rotate(135deg); 	/* IE 9 */
-moz-transform:rotate(135deg); 	/* Firefox */
-webkit-transform:rotate(135deg); /* Safari 和 Chrome */
-o-transform:rotate(135deg); 	/* Opera */}

.close_1 .lap_close{background:#2ea7e0;}
.close_1 .lap_close .close_left{background:#ffffff;}
.close_1 .lap_close .close_right{background:#ffffff;}

.close_2 .lap_close{background:#FFFFFF;}
.close_2 .lap_close .close_left{background:#9B9B9B;}
.close_2 .lap_close .close_right{background:#9B9B9B;}

.close_3 .lap_close{background:#FFFFFF;}
.close_3 .lap_close .close_left{background:#FFFFFF;}
.close_3 .lap_close .close_right{background:#FFFFFF;}
.overlap .lap_block .lap_content  .close_3 .lap_close{background:url(../../images/common/overlap_close_white.png)no-repeat center;}
.overlap .lap_block .lap_content  .close_4 .lap_close{background:url(../../images/common/overlap_close_blue.png)no-repeat center;}

