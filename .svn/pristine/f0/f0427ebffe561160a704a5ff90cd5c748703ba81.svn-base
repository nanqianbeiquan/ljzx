public class TaxplayerInfo extends BaseDimension implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 2443494921465554751L;
	
	public String getQymc() {
		return qymc;
	}

	public void setQymc(String qymc) {
		this.qymc = qymc;
	}

	public String getQssz() {
		return qssz;
	}

	public void setQssz(String qssz) {
		this.qssz = qssz;
	}

	public String getQsje() {
		return qsje;
	}

	public void setQsje(String qsje) {
		this.qsje = qsje;
	}

	public String getXjrq() {
		return xjrq;
	}

	public void setXjrq(String xjrq) {
		this.xjrq = xjrq;
	}

	public String getLevel() {
		return level;
	}

	public void setLevel(String level) {
		this.level = level;
	}

	/**
	 * 企业名称
	 */
	private String qymc;
	
	/**
	 * 税务类型
	 */
	private String qssz;
	/**
	 * 欠税于额
	 */
	private String qsje;
	
	/**
	 * 限缴日期
	 */
	private String xjrq;

	/**
	 * 事件等级
	 */
	private String level;
