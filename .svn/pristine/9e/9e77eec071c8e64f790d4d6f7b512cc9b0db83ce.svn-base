package com.srd.ljzd.dto.monitor;

import java.util.Date;


import java.util.List;

import com.srd.ljzd.entity.monitor.MonitorCompanyEvent;

public class MonitorComEventDTO extends MonitorCompanyEvent{

	private List<String> typeList;
	/**
	 * 最新更新日期
	 */
	private String currentDate;
	
	private String beginDate;

	private String dueDate;
	
	private String readStatus;
	/**
	 * 监控日期
	 */
	private String monitorDate;
	
	/**
	 * 日期周期: 0 最近七天 1 最近 15天
	 */
	private String dateCycle;
	
	private String infoShowDate;
	//监控公司id，对应monitorcompany表中的主键
	private String monitorCompanyId;

	public String getMonitorCompanyId() {
		return monitorCompanyId;
	}

	public void setMonitorCompanyId(String monitorCompanyId) {
		this.monitorCompanyId = monitorCompanyId;
	}

	public String getDateCycle() {
		return dateCycle;
	}

	public void setDateCycle(String dateCycle) {
		this.dateCycle = dateCycle;
	}

	public String getDueDate() {
		return dueDate;
	}

	public void setDueDate(String dueDate) {
		this.dueDate = dueDate;
	}

	public String getBeginDate() {
		return beginDate;
	}

	public void setBeginDate(String beginDate) {
		this.beginDate = beginDate;
	}

	public String getMonitorDate() {
		return monitorDate;
	}

	public void setMonitorDate(String monitorDate) {
		this.monitorDate = monitorDate;
	}

	public String getInfoShowDate() {
		return infoShowDate;
	}

	public void setInfoShowDate(String infoShowDate) {
		this.infoShowDate = infoShowDate;
	}

	public String getCurrentDate() {
		return currentDate;
	}

	public void setCurrentDate(String currentDate) {
		this.currentDate = currentDate;
	}

	public List<String> getTypeList() {
		return typeList;
	}

	public void setTypeList(List<String> typeList) {
		this.typeList = typeList;
	}

	public String getReadStatus() {
		return readStatus;
	}

	public void setReadStatus(String readStatus) {
		this.readStatus = readStatus;
	}
	
}
