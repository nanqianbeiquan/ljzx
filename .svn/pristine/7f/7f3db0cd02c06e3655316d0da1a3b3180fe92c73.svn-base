.body_block {
	padding:15px 0px 15px 20px;
}
.body_block_title{
	font-size:16px;
	font-weight:700;
	color:#666666;
	padding:0px;
}
#AccountInfoBlock{
	margin:10px 0px 6px 0px;
}
#AccountInfoBlock .account{
	width:120px;
	height:156px;
	box-shadow: 2px 2px 12px #d7d7d7;
	cursor:pointer;
}
#AccountInfoBlock .account .account_icon span{
	position:relative;
	z-index:1000;
	color:#ffffff;
	font-size:12px;
	left:2px;
	top:-2px
}
#AccountInfoBlock .account .account_num{
	font-size:14px;
	text-align: center;
	margin:12px 0px 8px 0px;
}
#AccountInfoBlock .account .account_percent{
	font-size:24px;
	text-align: center;
	color:#666666;
	margin:0px 0px 27px 0px;
}
#AccountInfoBlock .account .account_name{
	font-size:12px;
	color:#666666;
	font-weight:700;
	text-align: center;
}
#AccountInfoBlock .account .account_index{
	font-size:12px;
	color:#666666;
	text-align: center;
	line-height: 20px;
	font-weight:700;
}
#kaka{
	position: absolute;
	right: 0px;
	width: 18px;
	text-align: center;
	
}
#AccountInfoBlock .account .position_name{
	position: absolute;	
	width:80px;
	margin-left:20px;
	overflow:hidden;
	text-overflow:ellipsis;
	bottom:1px;
	font-size:12px;
	color:#666666;
	text-align: center;
	line-height: 20px;
	font-weight:700;
	
	
}
#AccountInfoBlock .account .account_index_right{
	position: relative;
    left: 0px;   
    text-align: center;
    color: #ffffff;
    top: -4px;
    font-size: 12px;
}


#AccountInfoBlock .account_back_0{
	background:url(../../../images/modules/account/account_back_0.png) no-repeat 0px 80px;cursor: default;
}
