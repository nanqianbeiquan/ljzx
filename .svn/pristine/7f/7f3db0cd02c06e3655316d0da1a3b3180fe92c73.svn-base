}
#AccountMonitorCompany .monitor_level_3{
	background:#e95353;
}
#AccountMonitorCompany .account_level{
	margin:5px 0px 0px 0px;
	position: relative;
    top: 20px;
}
#AccountMonitorCompany .account_list{
	min-width:336px;
	max-width: 970px;
    vertical-align: top;
    padding: 0px 32px;
    margin: 10px 0px 0px 25px;
}
#AccountMonitorCompany .account_list:after{
	content:"";
	display: block;
	width:100%;
	border-top:1px solid #edeef2;
	position:relative;
	top:-39px;
	left:-32px;
	padding:0px 32px;
}
#AccountMonitorCompany .account_list .account{
	text-align: center;
	width:84px;
	cursor: pointer;
	margin: 20px 0px 0px 0px;
}
#AccountMonitorCompany .account_list .account .monitor_company_back{
	width:10px;
	height:50px;
	border-top-right-radius: 25px;
	border-top-left-radius: 25px;
	background:#edeef2;
	position:relative;
	left:7px;
}
#AccountMonitorCompany .account_list .account .monitor_company_percent{
	background:#1aa6fa;
	height:18px;
	width:10px;
	border-top-right-radius: 25px;
	border-top-left-radius: 25px;
	position:relative;
	left:-7px;
	vertical-align:bottom;
}
#AccountMonitorCompany .account_list .account .monitor_company_percent .percent_value_block{
	top:-21px;
	left:-14px;
	display: none;
}
#AccountMonitorCompany .account_list .account:hover .monitor_company_percent .percent_value_block{
	display: block;
}
#AccountMonitorCompany .account_list .account .monitor_company_percent .percent_value{
	font-size:12px;
	color:#1aa6fa;
	border:1px solid #1aa6fa;
	text-align: center;
	height:13px;
	line-height: 13px;
	width:34px;
	border-radius: 2px;
}
#AccountMonitorCompany .account_list .account .monitor_company_percent .percent_icon{
	width: 0;
    height: 0;
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-top: 4px solid #18A5FA;
    position: relative;
    left: 15px;
}
#AccountMonitorCompany .account_list .account .monitor_company_percent .percent_icon_blank{
	width: 0;
    height: 0;
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-top: 4px solid #eeffff;
    position: relative;
    left: -4px;
    top: -5px;
}
