    border-top: 30px solid #fec414;
    border-right: 30px solid transparent;
}

#AccountInfoBlock #AccountListLeftBtn{
	background:url(../../../images/modules/account/left_btn.png) no-repeat 0px 0px;
	border-top-left-radius: 50%;
	border-bottom-left-radius: 50%;
	width: 20px;
    height: 36px;
    cursor: pointer;
    margin: 0px 25px 0px 23px;
    
}
#AccountInfoBlock #AccountListLeftBtn:hover{
	background:url(../../../images/modules/account/left_btn_hover.png) no-repeat 0px 0px;
}
#AccountInfoBlock #AccountListRightBtn{
	background:url(../../../images/modules/account/right_btn.png) no-repeat 0px 0px;
	border-top-right-radius: 50%;
	border-bottom-right-radius: 50%;
	width: 20px;
    height: 36px;
	cursor: pointer;
    margin: 0px 22px 0px 24px;
}
#AccountInfoBlock #AccountListRightBtn:hover{
	background:url(../../../images/modules/account/right_btn_hover.png) no-repeat 0px 0px;
}
#AccountInfoBlock #AccountList{
	width:903px;
	overflow: hidden;
